programa {
  funcao inicio() {
    //Faça um algoritmo que leia 5 números e informe a soma e a média dos números.

    
    inteiro numero1, numero2,numero3,numero4,numero5,soma,resultado_soma
     inteiro media
     para (soma)m{
       soma=numero1+numero2+numero3+numero4+numero5
     }
      para(media) {
        media=resultado_soma/5

      }

     escreva("A soma do numero inserido é ",soma)
     escreva("A media dos numeros inseridos é",media)
  }
}
